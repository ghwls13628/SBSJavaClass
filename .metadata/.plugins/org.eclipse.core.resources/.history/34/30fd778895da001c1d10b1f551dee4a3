package Java2Class5;

public class Scanner {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num1 = sc.nextInt(); 
		int num2 = sc.nextInt(); 
		int num3 = sc.nextInt(); 
		
		int sum = num1 + num2 + num3;
		System.out.println("%d + %d + %d = %d \n", num1, num2, num3, sum);	

	}

}
